{
  "manifest_version": 3,
  "name": "Cheat Sheet",
  "version": "1.0",
  "description": "Save highlight to Cheat Sheet",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://localhost:8888/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
